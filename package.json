{
  "name": "university-voting-blockchain",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "npm run compile:contracts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "predev": "npm run compile:contracts",
    "compile:contracts": "hardhat compile && npm run generate:abi",
    "generate:abi": "node scripts/generate-abi.js",
    "hh:compile": "hardhat compile && npm run generate:abi",
    "hh:test": "hardhat test",
    "hh:deploy:baseSepolia": "hardhat run scripts/deploy.js --network baseSepolia",
    "deploy:university": "hardhat run scripts/deploy-university.js --network baseSepolia",
    "deploy:local": "hardhat run scripts/deploy-university.js --network localhost",
    "start:node": "hardhat node",
    "install:deps": "npm install --force",
    "install:ci": "npm ci",
    "setup": "npm install && hardhat compile && npm run generate:abi",
    "dev:no-contracts": "next dev",
    "dev:force": "npm run setup && npm run dev",
    "compile:force": "npm install --force && hardhat compile && npm run generate:abi",
    "clean:deps": "npx -y rimraf node_modules package-lock.json pnpm-lock.yaml && npm cache clean --force",
    "fix:deps": "npm run clean:deps && npm install --force",
    "install:clean": "npm run clean:deps && npm install",
    "hardhat:version": "hardhat --version",
    "test:contracts": "hardhat test",
    "clear-cache": "node scripts/clear-cache-and-rebuild.js",
    "validate-system": "node scripts/validate-full-system.js",
    "fresh-start": "npm run clear-cache -- --clear-lockfile --clear-node-modules && npm run validate-system",
    "build-fresh": "npm run clear-cache && npm run build"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@tanstack/react-query": "^5.85.6",
    "@wagmi/core": "^2.5.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "ethers": "^6.14.0",
    "lucide-react": "^0.344.0",
    "next": "14.2.5",
    "next-themes": "^0.4.6",
    "pino-pretty": "^13.1.1",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "viem": "^2.7.9",
    "wagmi": "^2.5.7",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.6",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "chai": "^4.5.0",
    "dotenv": "^17.2.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "hardhat": "2.26.3",
    "mocha": "^10.2.0",
    "postcss": "^8",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
